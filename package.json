    "@types/browser-sync": "2.26.1",
    "@types/chokidar": "2.1.3",
    "@types/fibers": "3.1.0",
    "@types/node": "14.14.25",
    "@types/pify": "5.0.0",
    "@types/sass": "1.16.0",
    "@types/yargs": "16.0.0",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "cpy-cli": "3.1.1",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  }
